{
  "e2e": {
    "stepDefinitions": "cypress/step_definitions/**/*.{js, ts}",
    "nonGlobalStepDefinitions": true
  },
  "pretty": {
    "enabled": true
  },
  "json": {
    "enabled": true,
    "output": "cypress/results/json/cucumber-json.json"
  },
  "messages": {
    "enabled": true,
    "output": "cypress/results/messages/cucumber-messages.ndjson"
  },
  "html": {
    "enabled": true,
    "output": "cypress/results/html/cucumber-html.html"
  }
}
